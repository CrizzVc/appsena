<div id="persons">
  <p>
    <strong>Name:</strong>
    <%= personaje.name %>
  </p>

  <p>
    <strong>Lastname:</strong>
    <%= personaje.lastname %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= personaje.age %>
  </p>

  <p>
    <strong>Serie:</strong>
    <%= personaje.serie %>
  </p>

  <img src="<%= personaje.img %>" width="120"></img>

  <p>
    <%= link_to "Show this personaje", personaje %>
  </p>

</div>
